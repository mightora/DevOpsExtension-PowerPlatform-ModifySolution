{
    "manifestVersion": 1,
    "id": "mightora-dev-power-platform-solution-modifier-dev",
    "name": "DEV - Mightora.io Power Platform Solution Modifier",
    "version": "1.0.82",
    "publisher": "mightoraio",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Power Platform Solution Modifier Extension, allows users to modify XML nodes and attributes in Power Platform solution files during CI/CD pipelines.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/mightora_io_icon_260x260.png"
    },
    "content": {
        "details": {
            "path": "readme.md"
        },
        "pricing": {
            "path": "pricing.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://mightora.io"
        },
        "getstarted": {
            "uri": "https://mightora.io"
        },
        "learn": {
            "uri": "https://mightora.io"
        },
        "support": {
            "uri": "https://mightora.io"
        },
        "repository": {
            "uri": "https://mightora.io"
        },
        "issues": {
            "uri": "https://mightora.io"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/mightora"
    },
    "files": [
        {
            "path": "src/solutionXMLNodeUpdater",
            "packagePath": "solutionXMLNodeUpdater"
        },
        {
            "path": "src/solutionXMLNodeAttributeUpdater",
            "packagePath": "solutionXMLNodeAttributeUpdater"
        }
    ],
    "contributions": [
        {
            "id": "mightora-solutionXMLNodeUpdater-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "solutionXMLNodeUpdater"
            }
        },
        {
            "id": "mightora-solutionXMLNodeAttributeUpdater-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "solutionXMLNodeAttributeUpdater"
            }
        }
    ]
}